{
  "name": "temporal-workflows-webpack-plugin",
  "version": "1.0.0",
  "description": "Webpack plugin for bundling Temporal workflows with @temporalio/worker",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "prepublishOnly": "npm run build",
    "test": "echo \"Tests coming soon\" && exit 0"
  },
  "keywords": [
    "webpack",
    "webpack-plugin",
    "temporal",
    "temporalio",
    "workflow",
    "bundler"
  ],
  "author": "Oleksandr Sytnik <oleksandr.sytnikk@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sytnikk/temporal-workflows-webpack-plugin"
  },
  "peerDependencies": {
    "@temporalio/worker": "^1.0.0",
    "webpack": "^4.0.0 || ^5.0.0"
  },
  "dependencies": {
    "@temporalio/worker": "^1.13.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/node": "^24.10.0",
    "eslint": "^9.39.1",
    "prettier": "^3.6.2",
    "semantic-release": "^24.0.0",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.0.0",
    "webpack": "^5.0.0"
  }
}
